{
  "expo": {
    "name": "jobflow-maps",
    "slug": "jobflow-maps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.jobflow.maps",
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyCjntu12jwnP-TC-lC7zSp7d8BZMJETATg"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBXlAXX1HozMOJIz6hZ0S-JSkuaV74IlMg"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a42dd243-c2fd-4699-b131-f0b4125f33f4"
      }
    },
    "owner": "skander.blaiti"
  }
}
